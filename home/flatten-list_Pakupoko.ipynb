    "def flat_list(array): \n",
    "    a=[]\n",
    "    for i in array:\n",
    "        if isinstance(i,list):\n",
    "            a+=flat_list(i)       \n",
    "        else:\n",
    "            a.append(i)            \n",
    "    return a"
   ]
