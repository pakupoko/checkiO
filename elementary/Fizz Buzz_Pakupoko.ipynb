    "def checkio(number: int) -> str:\n",
    "    \n",
    "    if number % 3 == 0 and number % 5 == 0:\n",
    "        return 'Fizz Buzz'\n",
    "    \n",
    "    elif number % 3 == 0 :\n",
    "        return 'Fizz'\n",
    "    \n",
    "    elif number % 5 == 0 :\n",
    "        return 'Buzz'\n",
    "    \n",
    "    else:\n",
    "        return str(number)"
